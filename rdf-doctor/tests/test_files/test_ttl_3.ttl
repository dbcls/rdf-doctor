@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ex: <http://example.org/> .

ex:artwork1 rdf:type ex:Artwork ;
    ex:creator "Vincent van Gogh" ;
    ex:title "Starry Night" ;
    ex:creationYear "1889" ;
    ex:museum ex:museum1 .

ex:museum1 rdf:type ex:Museum ;
    ex:name "Museum of Modern Art" ;
    ex:location "11 W 53rd St, New York, NY 10019" .

ex:artwork2 rdf:type ex:Artwork ;
    ex:creator "Leonardo da Vinci" ;
    ex:title "Mona Lisa" ;
    ex:creationYear "1503-1519" ;
    ex:museum ex:museum2 .

ex:museum2 rdf:type ex:Museum ;
    ex:name "Louvre Museum" ;
    ex:location "Rue de Rivoli, 75001 Paris, France" .
