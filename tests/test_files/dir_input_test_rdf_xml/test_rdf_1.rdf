  </foaf:Person>

  <rdf:Description rdf:about="http://example.org/HumanLike">
    <foaf:name>Person</foaf:name>
    <foaf:familyName>Maybe</foaf:familyName>
    <foaf:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">99</foaf:age>
    <foaf:knows rdf:resource="http://example.org/David"/>
  </rdf:Description>

  <foaf:Document rdf:about="http://example.org/x1">
    <foaf:depiction>A thing that s nice</foaf:depiction>
    <foaf:title>A nice thing</foaf:title>
  </foaf:Document>

  <foaf:Document rdf:about="http://example.org/x2">
    <foaf:title>Another thing</foaf:title>
  </foaf:Document>

